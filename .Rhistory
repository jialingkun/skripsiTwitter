search <- function(searchterm)
{
#access tweets and create cumulative file
list <- searchTwitter(searchterm, n=300)
df <- twListToDF(list)
df <- df[, order(names(df))]
df$created <- strftime(df$created, '%Y-%m-%d')
if (file.exists(paste('D:/cumtw.csv'))==FALSE) write.csv(df, file=paste('D:/cumtw.csv'), row.names=F)
#merge last access with cumulative file and remove duplicates
stack <- read.csv(file=paste('D:/cumtw.csv'))
stack <- rbind(stack, df)
stack <- subset(stack, !duplicated(stack$text))
write.csv(stack, file=paste('D:/cumtw.csv'), row.names=F)
}
